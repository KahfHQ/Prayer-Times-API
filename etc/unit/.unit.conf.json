{
  "listeners": {
    "*:8080": {
      "pass": "routes/main"
    }
  },
  "applications": {
    "api.aladhan.com": {
      "type": "php",
      "user": "nobody",
      "processes": 2,
      "root": "/var/www/html",
      "index": "index.php",
      "script": "index.php"
    }
  },
  "routes": {
    "main": [
      {
        "match": {
          "uri": [
            "/timings*",
            "/calendar*",
            "/address*",
            "/city*",
            "/g*",
            "/h*",
            "/methods",
            "/asmaAlHusna",
            "/qibla"
          ]
        },
        "action": {
          "return": 301,
          "location": "/v1$request_uri"
        }
      },
      {
        "match": {
          "uri": "*"
        },
        "action": {
          "pass": "applications/api.aladhan.com"
        }
      }
    ]
  },
  "access_log": {
    "path": "/dev/stdout"
  }
}
